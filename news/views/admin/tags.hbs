<div class=" table-responsive">
    <table class="table table-hover">
        <thead>
            <tr>
                <th scope="col">Tag Id</th>
                <th scope="col">Title</th>
            </tr>
        </thead>
        <tbody>
            {{#each this.tags}}
            <tr>
                <th scope="row">{{TagId}}</th>
                <td>{{TagName}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>